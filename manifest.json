{
  "manifest_version": 2,
  "name": "Latex From Web",
  "description": "This extension add a button on Latex rendered formulas in Web page to copy on fly the Latex code.",
  "version": "1.0.1",
  "version_name": "1.0.1 beta",
  "author": "Simone Scardoni",
  "icons" : {
    "16" : "images/16-aico.png",
    "48" : "images/48-aico.png",
    "128" : "images/128-aico.png"
  },
  "web_accessible_resources" : [
    "style/*"
  ],
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Open Options",
    "default_icon": {
      "19" : "images/19-iico.png",
      "38" : "images/38-iico.png"
    }
  },
  "options_page": "copy_latex_from_web_options.html",
  "options_ui": {
      "page": "copy_latex_from_web_options.html",
      "chrome_style": true
    },
  "background" : {
    "scripts": ["lib/jquery-1.12.3.min.js","lib/classes.js","background.js"]
  }
}
